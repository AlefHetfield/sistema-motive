 {
   "version": 2,
   "buildCommand": "npm run vercel-build",
   "builds": [
     {
       "src": "api/server.js",
       "use": "@vercel/node"
     },
     {
       "src": "package.json",
       "use": "@vercel/static-build",
       "config": { "distDir": "." }
     }
   ],
   "routes": [
     {
       "src": "/api/(.*)",
       "dest": "/api/server.js"
     },
     {
       "src": "/(.*)",
       "dest": "/$1"
     }
   ]
 }